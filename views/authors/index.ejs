<h1>Authors</h1>

<% authors.forEach(function(author) { %>
  <div class="well">
    <h2>
      <a href="/authors/<%= author.id %>">
        <%= `${author.firstName} ${author.lastName}` %>
      </a>
    </h2>
  </div>
<% }) %>

<a href="/authors/new">Sign Up</a>

<hr />

